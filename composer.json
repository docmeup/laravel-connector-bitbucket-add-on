{
    "name": "docmeup/laravel-connector-bitbucket",
    "description": "Creates ready-to-go bitbucket app connector endpoints",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Mills",
            "email": "tylerssn@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.8.*",
        "firebase/php-jwt": "5.0.*"
    },
    "autoload": {
        "psr-4": {
            "DocMeUp\\Connector\\Bitbucket\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DocMeUp\\Connector\\Bitbucket\\App\\Providers\\ConfigPublishProvider",
                "DocMeUp\\Connector\\Bitbucket\\App\\Providers\\RouteServiceProvider"
            ]
        }
    }
}
